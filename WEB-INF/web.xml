<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instanjavax jsonce" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<display-name>CORSFilter</display-name>

<servlet>
  <description></description>
  <display-name>CORSFilter</display-name>
  <servlet-name>CORSFilter</servlet-name>
  <servlet-class>cors.CORSFilter</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>CORSFilter</servlet-name>
  <url-pattern>/note</url-pattern>
</servlet-mapping>

<servlet>
  <description></description>
  <display-name>DataStart</display-name>
  <servlet-name>DataStart</servlet-name>
  <servlet-class>storage.DataStart</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>DataStart</servlet-name>
  <url-pattern>/service</url-pattern>
</servlet-mapping>

<welcome-file-list>
  <welcome-file>index.html</welcome-file>
  <welcome-file>index.xhtml</welcome-file>
  <welcome-file>index.htm</welcome-file>
  <welcome-file>index.jsp</welcome-file>
</welcome-file-list>
</web-app>